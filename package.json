{
  "name": "btpdemo",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap/cds": "^8",
    "@sap/xssec": "^4",
    "express": "^4"
  },
  "cds": {
    "requires": {
      "db": {
        "kind": "sqlite",
        "credentials": {
          "url": "btpdemodb"
        }
      },
      "insurance_claim": {
        "kind": "odata",
        "model": "srv/external/insurance_claim"
      },
      "uaa" : {
        "kind": "xsuaa"
      },
      "auth" : {
        "[production]" : {
          "strategy" : "mock",
          "users": {
            "prasanna" : {
              "roles": ["Admin", "Viewer"]
            },
            "saurav" : {
              "roles": ["Viewer"]
            }
          }
        },
        "[development]" : {
          "strategy" : "mock",
          "users": {
            "prasanna" : {
              "roles": ["Admin", "Viewer"]
            },
            "saurav" : {
              "roles": ["Viewer"]
            }
          }
        },
      }
    }
  },
  "devDependencies": {
    "@cap-js/cds-types": "^0.6",
    "@cap-js/sqlite": "^1.7.3",
    "@sap/cds-dk": ">=7",
    "@sap/ux-specification": "^1.120.14"
  },
  "scripts": {
    "start": "cds-serve",
    "watch-genericfoodmodule": "cds watch --open genericfoodmodule/webapp/index.html?sap-ui-xx-viewCache=false"
  },
  "sapux": [
    "app/genericfoodmodule"
  ]
}
